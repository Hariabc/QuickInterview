{
  "name": "quickinterview-ai",
  "version": "1.0.0",
  "description": "AI-powered interview assistant with resume parsing, real-time scoring, and cross-tab synchronization",
  "type": "module",
  "keywords": [
    "ai",
    "interview",
    "assistant",
    "react",
    "groq",
    "resume-parsing",
    "real-time",
    "cross-tab-sync"
  ],
  "author": "QuickInterview.ai Team",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/QuickInterview#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/QuickInterview.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/QuickInterview/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "idb": "^7.1.1",
    "mammoth": "^1.6.0",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0"
  }
}
